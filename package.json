{
  "name": "ffmpeg-guide-creator",
  "version": "1.0.0",
  "description": "An Electron application to split QuickTime videos based on Adobe Premiere Pro chapter markers.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=logo/logo.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . --overwrite --platform=win32 --arch=ia32 --icon=logo/logo.ico --prune=true --out=release-builds"
  },
  "keywords": [
    "ffmpeg",
    "electron",
    "video",
    "chapters",
    "premiere pro"
  ],
  "author": "iori/gemini",
  "license": "MIT",
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-packager": "^17.1.2"
  },
  "dependencies": {
    "fluent-ffmpeg": "^2.1.2"
  }
}
